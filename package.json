    "nouislider-react": "^3.4.1",
    "react": "^17.0.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-animation-on-scroll": "^2.1.0",
    "react-chartjs-2": "^4.0.0",
    "react-countup": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.1.0",
    "react-scripts": "4.0.3",
